{
  "name": "@sendo-labs/plugin-sendo-analyser",
  "description": "Sendo Analyser plugin for ElizaOS - provides blockchain wallet analysis and insights using sendo-api",
  "version": "1.6.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sendo-labs/plugin-sendo-analyser.git"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "dependencies": {
    "@coral-xyz/borsh": "^0.31.1",
    "@elizaos/core": "^1.6.3",
    "@solana/buffer-layout": "^4.0.1",
    "@solana/buffer-layout-utils": "^0.3.0",
    "@solana/web3.js": "^1.98.4",
    "@types/bn.js": "^5.2.0",
    "@types/pg": "^8.15.5",
    "axios": "^1.12.2",
    "bn.js": "^5.2.2",
    "bs58": "^6.0.0",
    "drizzle-orm": "^0.44.6",
    "helius-sdk": "^2.0.2",
    "pg": "^8.16.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@electric-sql/pglite": "^0.3.11",
    "@types/bun": "^1.3.0",
    "@types/node": "^24.7.0",
    "knip": "^5.68.0",
    "prettier": "3.5.3",
    "ts-prune": "^0.10.3",
    "tsup": "8.5.0",
    "typescript": "5.9.3"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean:dist": "rm -rf dist .turbo node_modules tsconfig.tsbuildinfo",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "prettier --check ./src",
    "check": "biome check --write .",
    "check:dead-code": "knip",
    "check:exports": "ts-prune",
    "test": "elizaos test",
    "test:unit": "bun test --bail src/__tests__/unit/*.test.ts",
    "test:integration": "bun test --bail src/__tests__/integration/*.test.ts",
    "test:integration:llm": "USE_REAL_LLM=true bun run test:integration",
    "test:all": "bun run test:unit && bun run test:integration",
    "publish": "elizaos plugin publish"
  },
  "platform": "universal",
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "sendoApiUrl": "http://localhost:3000/api/v1"
    }
  }
}
